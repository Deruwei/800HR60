<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="@dimen/x100">
    <RelativeLayout
        android:id="@+id/rl_ResumeItem"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x100" >
        <TextView
            android:id="@+id/tv_resumeItemName"
            android:layout_toLeftOf="@+id/tv_resumeItemName2"
            android:textColor="@color/color_333"
            android:layout_marginLeft="@dimen/x31"
            android:layout_centerVertical="true"
            android:textSize="@dimen/x36"
            android:layout_marginRight="@dimen/x31"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/tv_resumeItemName2"
            android:layout_toLeftOf="@+id/iv_resumeItemSelect"
            android:textColor="@color/color_333"
            android:textSize="@dimen/x28"
            android:layout_marginRight="@dimen/x12"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:id="@+id/iv_resumeItemSelect"
            android:layout_alignParentRight="true"
            android:src="@mipmap/right_arrow"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/x24"
            android:layout_width="@dimen/x11"
            android:layout_height="@dimen/y18" />
        <View
            android:layout_alignParentBottom="true"
            android:background="@color/view_color"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"/>
    </RelativeLayout>
</LinearLayout>