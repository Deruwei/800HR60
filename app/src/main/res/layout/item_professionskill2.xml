<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_marginLeft="@dimen/x54"
        android:layout_marginRight="@dimen/x54"
        android:layout_marginTop="@dimen/x32"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tv_previewResumeProfessionSkillName"
        android:textColor="@color/color_333"
        android:textSize="@dimen/x30"
        android:text="@string/shieldCompanyAll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_below="@+id/tv_previewResumeProfessionSkillName"
        android:id="@+id/tv_previewResumeProfessionSkillUseTime"
        android:textColor="@color/color_333"
        android:textSize="@dimen/x30"
        android:layout_marginTop="@dimen/x6"
        android:text="@string/shieldCompanyAll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_below="@+id/tv_previewResumeProfessionSkillName"
        android:layout_toRightOf="@+id/tv_previewResumeProfessionSkillUseTime"
        android:layout_marginLeft="@dimen/x54"
        android:id="@+id/tv_previewResumeProfessionSkillLevel"
        android:textColor="@color/color_333"
        android:textSize="@dimen/x30"
        android:layout_marginTop="@dimen/x6"
        android:text="@string/shieldCompanyAll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    </RelativeLayout>
</LinearLayout>